\name{autolayout}
\alias{autolayout}
\title{Set the layout when making multiple plots on the same page.}
\usage{
  autolayout(n)
}
\arguments{
  \item{n}{number of plots to make}
}
\value{
  number of rows and columns in the layout
}
\description{
  Utility function to set the layout when making multiple
  plots on the same page.
}
\author{
  Zhenglei Gao
}

